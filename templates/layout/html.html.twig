{% extends '@localgov_base/layout/html.html.twig' %}
{% block extra_head_items %}
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
  <link rel="preload" as="style" href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@400;600;700;800&display=fallback" />
  <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@400;600;700;800&display=fallback" media="print" onload="this.media='all'" />
  <noscript>
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@400;600;700;800&display=fallback" />
  </noscript>
  <link rel="mask-icon" href="/{{ base_path ~ directory }}/assets/images/favicons/safari-pinned-tab.svg" color="#005e7a">
  <meta name="msapplication-TileColor" content="#005e7a">
  <meta name="msapplication-config" content="/{{ base_path ~ directory }}/assets/images/favicons/browserconfig.xml">
  <meta name="theme-color" content="#9fc9d6">
{% endblock %}
